#include <stdio.h>
int main() 
{
    int n; 
    printf("Enter the number of elements: ");
    scanf("%d", &n);
    int arr1[n]; 
    int arr2[n]; 
    printf("Enter %d elements for the original array:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr1[i]);
    }
    for (int i = 0; i < n; i++) 
    printf("Original array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr1[i]);
    }
    printf("\n");
    printf("Copy array: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", arr2[i]);
    }
    printf("\n");
    return 0;
}
